    {{!-- Logout Fail Notification Modal --}}
    <div class="modal fade" id="logoutFailModal" tabindex="-1" role="dialog" aria-labelledby="logoutFailModalTitle"
      aria-hidden="true">
      <div class="modal-dialog modal-dialog-centered" role="document">
        <div class="modal-content">
          <div class="modal-header">
            <h5 class="modal-title" id="logoutFailModalTitle">Logout Failed.</h5>
            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
              <span aria-hidden="true">&times;</span>
            </button>
          </div>
          <div class="modal-body">
            <p>Please try again.</p>
          </div>
          <div class="modal-footer">
            <button type="button" class="btn btn-secondary" data-dismiss="modal">OK</button>
          </div>
        </div>
      </div>
    </div>


    <footer>
      <p>Come see us soon!</p>
    </footer>


